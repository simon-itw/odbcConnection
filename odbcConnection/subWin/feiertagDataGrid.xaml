<Window x:Class="odbcConnection.subWin.feiertagDataGrid"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:odbcConnection.subWin"
        mc:Ignorable="d"
        Title="feiertagDataGrid" Height="450" Width="800">
    <Grid>
        <DataGrid Height="350" Width="700" VerticalAlignment="Center" Margin="15" Name="dataGridFeiertage">
            <DataGrid.Columns>
                <!--<DataGridTextColumn Binding="{Binding Datum,StringFormat={}{0:dd.MM.yyyy}}" Width="Auto"/>
                <DataGridTextColumn Header="Name" Binding="{Binding Name}" Width="Auto"/>-->
            </DataGrid.Columns>
        </DataGrid>

        <Button Content="Zurück" Height="20" Width="Auto" Margin="3" VerticalAlignment="Top" HorizontalAlignment="Center" Click="Btn_Back"/>

    </Grid>
</Window>
